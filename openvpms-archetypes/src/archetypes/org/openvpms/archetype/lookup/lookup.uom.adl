<archetypes>
	<archetype name="openvpms-lookup-lookup.uom.1.0" latest="true"
		type="org.openvpms.component.business.domain.im.lookup.Lookup"  displayName="Units of Measure" >
		<node name="uid" path="/uid" displayName="id" type="java.lang.Long" hidden="true" readOnly="true" />
		<node name="uom" path="/value" type="java.lang.String" minCardinality="1" />
		<node name="name" type="java.lang.String" path="/name"
			hidden="true" minCardinality="1" derived="true"
			derivedValue="/value" />
		<node name="description" type="java.lang.String"
			path="/description" hidden="true" derived="true"
			derivedValue="concat(/value,lookup:isDefault(/defaultLookup))" />
		<!-- node name="target" path="/targetLookupRelationships"
			type="java.util.HashSet" baseName="targetLookupRelationship" minCardinality="0" maxCardinality="*">
			<assertion name="archetypeRange" filter="lookupRelationship.uomGroup" >
				<propertyList name="archetypes">
					<propertyMap name="archetype">
						<property name="shortName"
							value="lookupRelationship.uomGroup" />
					</propertyMap>
				</propertyList>
				<errorMessage>errorMessage</errorMessage>
			</assertion>
		</node-->
		<node displayName="default" name="defaultLookup" type="java.lang.Boolean" path="/defaultLookup" defaultValue="false()" /> 
	</archetype>
</archetypes>
