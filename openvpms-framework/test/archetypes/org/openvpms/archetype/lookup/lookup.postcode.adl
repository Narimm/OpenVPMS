<archetypes>
	<archetype name="openvpms-lookup-lookup.postcode.1.0" latest="true"
		type="org.openvpms.component.business.domain.im.lookup.Lookup" displayName="Postcode" >
		<node name="uid" path="/uid" displayName="id" type="java.lang.Long" readOnly="true" />
		<node name="postCode" path="/value" type="java.lang.String" minCardinality="1" />
		<node name="name" type="java.lang.String" path="/name"
			hidden="true" minCardinality="1" derived="true"
			derivedValue="/value" />
		<node name="description" type="java.lang.String"
			path="/description" hidden="true" derived="true"
			derivedValue="/value" />
		<node name="defaultLookup" type="java.lang.Boolean" path="/defaultLookup"
			defaultValue="false()" />
        <node name="target" path="/targetLookupRelationships"
			type="java.util.HashSet" baseName="targetLookupRelationship" minCardinality="0" maxCardinality="*">
			<assertion name="archetypeRange">
				<propertyList name="archetypes">
					<propertyMap name="archetype">
						<property name="shortName"
							value="lookupRelationship.suburbPostcode" />
					</propertyMap>
				</propertyList>
				<errorMessage>errorMessage</errorMessage>
			</assertion>
		</node>
        <node name="source" path="/sourceLookupRelationships"
			type="java.util.HashSet" baseName="sourceLookupRelationship" minCardinality="0" maxCardinality="*"/>
	</archetype>
</archetypes>
