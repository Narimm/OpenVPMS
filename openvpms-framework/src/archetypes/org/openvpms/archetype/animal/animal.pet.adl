<archetypes>
	<archetype name="openvpms-party-animal.pet.1.0" latest="true"
		type="org.openvpms.component.business.domain.im.party.Animal">
		<node name="uid" path="/uid" displayName="id" type="java.lang.Long" />
		<node name="name" path="/name" type="java.lang.String"
			minCardinality="1" />
		<node name="species" path="/species" type="java.lang.String"
			minCardinality="1">
			<assertion name="lookup">
				<property name="type" value="targetLookup" />
				<property name="concept" value="species" />
				<errorMessage>An error message</errorMessage>
			</assertion>
		</node>
		<node name="breed" path="/breed" type="java.lang.String">
			<assertion name="lookup">
				<property name="type" value="targetLookup" />
				<property name="concept" value="species.breed" />
				<property name="source" value="species" />
				<errorMessage>An error message</errorMessage>
			</assertion>
		</node>
		<node name="colour" path="/colour" type="java.lang.String">
			<assertion name="lookup">
				<property name="type" value="targetLookup" />
				<property name="concept" value="breed.colour" />
				<property name="source" value="breed" />
				<errorMessage>An error message</errorMessage>
			</assertion>
		</node>
		<node name="sex" path="/sex" type="java.lang.String">
			<assertion name="lookup">
				<property name="male" value="male" />
				<property name="female" value="female" />
				<property name="unspecified" value="unspecified" />
				<errorMessage>errorMessage</errorMessage>
			</assertion>
		</node>
		<node name="desexed" path="/desexed" type="java.lang.Boolean"
			defaultValue="false()" />
		<node name="dateOfBirth" path="/dateOfBirth"
			type="java.util.Date" minCardinality="1" />
		<node name="identities" path="/identities"
			type="org.openvpms.component.business.domain.im.common.EntityIdentity">
			<assertion name="archetypeRange">
				<property name="entityIdentity.animalAlias" />
				<errorMessage>errorMessage</errorMessage>
			</assertion>
		</node>
		<node name="contacts" path="/contacts"
			type="org.openvpms.component.business.domain.im.party.Contact"
			maxCardinality="0" />

	</archetype>
</archetypes>