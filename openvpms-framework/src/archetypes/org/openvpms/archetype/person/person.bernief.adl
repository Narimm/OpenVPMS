<archetypes>
	<archetype name="openvpms-party-person.bernief.1.0" latest="true"
		type="org.openvpms.component.business.domain.im.party.Party">
		<node name="name" type="java.lang.String" path="/name"
			hidden="true" minCardinality="1" derived="true"
			derivedValue="concat(/details/attributes/lastName, ',', /details/attributes/firstName)" />
		<node name="description" type="java.lang.String"
			path="/description" hidden="true" derived="true"
			derivedValue="concat(/details/attributes/title, ' ', /details/attributes/firstName, ' ', /details/attributes/initials,' ',/details/attributes/lastName)" />
		<node name="details" path="/details" hidden="true"
			type="org.openvpms.component.business.domain.im.datatypes.basic.DynamicAttributeMap"
			minCardinality="1" maxCardinality="1">
			<node name="title" path="/details/attributes/title" type="java.lang.String"
				minCardinality="0">
				<assertion name="lookup.local">
					<propertyList name="entries">
						<property name="Mr" value="Mr" />
						<property name="Ms" value="Ms" />
						<property name="Mrs" value="Mrs" />
						<property name="Dr" value="Dr" />
						<property name="Miss" value="Miss" />
						<property name="Sir" value="Sir" />
						<property name="Lady" value="Lady" />
					</propertyList>
					<errorMessage>An error message</errorMessage>
				</assertion>
			</node>
			<node name="firstName" path="/details/attributes/firstName" type="java.lang.String"
				minCardinality="0" maxLength="30" />
			<node name="initials" path="/details/attributes/initials" type="java.lang.String" />
			<node name="lastName" path="/details/attributes/lastName" type="java.lang.String"
				minCardinality="1" maxLength="30" />
		</node>
		<node name="classifications" path="/classifications"
			parentChild="false" type="java.util.HashSet"
			baseName="classification" maxCardinality="*">
			<assertion name="archetypeRange">
				<propertyList name="archetypes">
					<propertyMap name="archetype">
						<property name="shortName"
							value="classification.staff" />
						<!--property name="defaultValue"
							value="staffMember1" /-->
					</propertyMap>
					<propertyMap name="archetype">
						<property name="shortName"
							value="classification.patient" />
						<property name="maxCardinality"
							value="1" />
					</propertyMap>
				</propertyList>
				<errorMessage>
					Invalid classifications specified
				</errorMessage>
			</assertion>
		</node>
    <node name="identities" type="java.util.HashSet" baseName="identity"
      path="/identities" maxCardinality="*">
      <assertion name="dummyAssertion1">
        <errorMessage>
          Error with assertion1
        </errorMessage>
      </assertion>
      <assertion name="dummyAssertion2">
        <errorMessage>
          Error with assertion2
        </errorMessage>
      </assertion>
      <assertion name="dummyAssertion3">
        <errorMessage>
          Error with assertion3
        </errorMessage>
      </assertion>
      <assertion name="dummyAssertion4">
        <errorMessage>
          Error with assertion4
        </errorMessage>
      </assertion>
      <assertion name="archetypeRange">
        <propertyList name="archetypes">
          <propertyMap name="archetype">
            <property name="shortName" value="entityIdentity.animalAlias" />
          </propertyMap>
          <propertyMap name="archetype">
            <property name="shortName" value="entityIdentity.personAlias" />
          </propertyMap>
          <propertyMap name="archetype">
            <property name="shortName" value="entityIdentity.customerAlias1" />
          </propertyMap>
          <propertyMap name="archetype">
            <property name="shortName" value="entityIdentity.customerAlias" />
          </propertyMap>
        </propertyList>
        <errorMessage>
          Idnetities must be of type entityIdentity.personAlias
        </errorMessage>
      </assertion>
    </node>

	</archetype>
</archetypes>
