	<parameter name="startDate" isForPrompting="true" class="java.util.Date"/>
	<parameter name="endDate" isForPrompting="true" class="java.util.Date"/>
	<queryString><![CDATA[select
  c.name as customer,
  c.description as description,
  p.name as patient,
  d1.value as dateOfBirth,
  cd.value as mobileNumber,
  a.activity_end_time as dueDate,
  rt.name as reminderType
from acts a
  join act_details ad on ad.act_id = a.act_id and ad.name = "reminderCount" 
  join participations pp on pp.act_id = a.act_id and pp.arch_short_name = "participation.patient"
  join entities p on pp.entity_id = p.entity_id
  join entity_relationships r on r.target_id=p.entity_id
  join entities c on r.source_id = c.entity_id
  join entity_details d1 on d1.entity_id = p.entity_id and d1.name = "dateOfBirth"
  join entity_details d2 on d2.entity_id = p.entity_id and d2.name = "desexed"
  join entity_details d3 on d3.entity_id = p.entity_id and d3.name = "deceased"
  join contacts cc on cc.party_id = c.entity_id
  join contact_details cd on cd.contact_id = cc.contact_id and cd.name = "mobileNumber" 
  join participations pr on pr.act_id = a.act_id and pr.arch_short_name = "particiaption.reminderType"
  join entities rt on rt.entity_id = pr.entity_id
where
  a.arch_short_name = "act.patientReminder" and
  a.activity_end_time >= $P{startDate} and
  date(a.activity_end_time) <= $P{endDate} and 
  p.arch_short_name like "party.patientpet%" and
  P.active = 1 and
  c.arch_short_name like "party.customer%" and
  c.active = 1 and
  cc.arch_short_name = "contact.mobileNumber" and
  d3.value = "false" and
  ad.value = "0"
order by c.name]]></queryString>

