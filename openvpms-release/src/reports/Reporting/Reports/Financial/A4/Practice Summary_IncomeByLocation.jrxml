	<queryString language="SQL">
		<![CDATA[select
(
select count(*) as customerCount
from entities ep
left join entity_details ed1 on ed1.entity_id = ep.entity_id and ed1.name = 'createdDate'
where ep.arch_short_name = 'party.customerperson'
and ep.active = 1
and date(ed1.value) < $P{DateFrom}
) as activeCustomersStart,
(
select count(*) as customersCount
from entities ep
left join entity_details ed1 on ed1.entity_id = ep.entity_id and ed1.name = 'createdDate'
where ep.arch_short_name = 'party.customerperson'
and ep.active = 1
and date(ed1.value) < DATE_ADD($P{DateTo}, INTERVAL "23:59:59" HOUR_SECOND)
) as activeCustomersEnd,
(
select count(distinct(c.entity_id)) as count from entities c
where c.active = 0
and c.arch_short_name = 'party.customerPerson'
) as inActiveCustomers,
(
select count(distinct pc.entity_id) as customerCount
from acts a
left join participations pc on pc.act_id = a.act_id and pc.arch_short_name = 'participation.customer'
left join entities ec on ec.entity_id = pc.entity_id
left join participations pl on pl.act_id = a.act_id and pl.arch_short_name = 'participation.location'
left join entities el on el.entity_id = pl.entity_id
where a.arch_short_name like 'act.customerAccountCharges%'
and a.activity_start_time between $P{DateFrom} and DATE_ADD($P{DateTo}, INTERVAL "23:59:59" HOUR_SECOND)
and a.status = 'POSTED'
) as totalCustomerCount,
(
select count(distinct pc.entity_id) as customerCount
from acts a
left join participations pc on pc.act_id = a.act_id and pc.arch_short_name = 'participation.customer'
left join entities ec on ec.entity_id = pc.entity_id
left join participations pl on pl.act_id = a.act_id and pl.arch_short_name = 'participation.location'
left join entities el on el.entity_id = pl.entity_id
left join participations pp on pp.entity_id = ec.entity_id
and pp.arch_short_name = 'participation.customer'
and pp.act_arch_short_name = 'act.customerAccountChargesInvoice'
and pp.activity_start_time < $P{DateFrom}
where a.arch_short_name like 'act.customerAccountCharges%'
and a.activity_start_time between $P{DateFrom} and DATE_ADD($P{DateTo}, INTERVAL "23:59:59" HOUR_SECOND)
and pp.entity_id is null
and a.status = 'POSTED'
) as totalNewCustomerCount,
count(distinct pc.entity_id) as customerCount, count(*) as transactionCount,
		sum(if(f.credit,-(f.total - f.tax_amount),(f.total - f.tax_amount)))as transactionTotalEx,
		sum(if(f.credit,-(f.total),(f.total)))as transactionTotalInc,
		sum(if(f.credit,-(f.tax_amount),(f.tax_amount)))as transactionTotalTax,
		cast(sum(if(a.arch_short_name = 'act.customerAccountChargesCredit',1,0)) as signed) as creditCount,
		sum(if(a.arch_short_name = 'act.customerAccountChargesCredit',if(f.credit,-(f.total - f.tax_amount),(f.total - f.tax_amount)),0))as creditTotalEx,
		sum(if(a.arch_short_name = 'act.customerAccountChargesCredit',if(f.credit,-(f.total),(f.total)),0))as creditTotalInc,
		sum(if(a.arch_short_name = 'act.customerAccountChargesCredit',if(f.credit,-(f.tax_amount),(f.tax_amount)),0))as creditTotalTax,
		cast(sum(if((a.arch_short_name = 'act.customerAccountChargesCounter')
			and (ec.arch_short_name = 'party.organisationOTC'),1,0)) as signed) as counterCount,
		sum(if((a.arch_short_name = 'act.customerAccountChargesCounter') and (ec.arch_short_name = 'party.organisationOTC'),
			if(f.credit,-(f.total - f.tax_amount),(f.total - f.tax_amount)),0))as counterTotalEx,
		sum(if((a.arch_short_name = 'act.customerAccountChargesCounter') and (ec.arch_short_name = 'party.organisationOTC'),
			if(f.credit,-(f.total),(f.total)),0))as counterTotalInc,
		sum(if((a.arch_short_name = 'act.customerAccountChargesCounter') and (ec.arch_short_name = 'party.organisationOTC'),
			if(f.credit,-(f.tax_amount),(f.tax_amount)),0))as counterTotalTax,
		cast(sum(if((a.arch_short_name = 'act.customerAccountChargesInvoice') or
			((a.arch_short_name = 'act.customerAccountChargesCounter') and (ec.name not like '%Counter Sales'))
			,1,0)) as signed) as invoiceCount,
		sum(if((a.arch_short_name = 'act.customerAccountChargesInvoice') or
			((a.arch_short_name = 'act.customerAccountChargesCounter') and (ec.name not like '%Counter Sales'))
			,if(f.credit,-(f.total - f.tax_amount),(f.total - f.tax_amount)),0))as invoiceTotalEx,
		sum(if((a.arch_short_name = 'act.customerAccountChargesInvoice') or
			((a.arch_short_name = 'act.customerAccountChargesCounter') and (ec.name not like '%Counter Sales'))
			,if(f.credit,-(f.total),(f.total)),0))as invoiceTotalInc,
		sum(if((a.arch_short_name = 'act.customerAccountChargesInvoice') or
			((a.arch_short_name = 'act.customerAccountChargesCounter') and (ec.name not like '%Counter Sales'))
			,if(f.credit,-( f.tax_amount),(f.tax_amount)),0))as invoiceTotalTax,
		ifnull(el.name,'--None--') as location
from acts a
left join participations pc on pc.act_id = a.act_id and pc.arch_short_name = 'participation.customer'
left join entities ec on ec.entity_id = pc.entity_id
left join participations pl on pl.act_id = a.act_id and pl.arch_short_name = 'participation.location'
left join entities el on el.entity_id = pl.entity_id
left join financial_acts f on a.act_id = f.financial_act_id
#left join participations pv on pv.act_id = a.act_id and pv.arch_short_name = 'participation.clinician'
#left join entities ev on ev.entity_id = pv.entity_id
where a.arch_short_name like 'act.customerAccountCharges%'
and a.activity_start_time between $P{DateFrom} and DATE_ADD($P{DateTo}, INTERVAL "23:59:59" HOUR_SECOND)
and a.status = 'POSTED'
and f.total != 0
group by location order by location;]]>
	</queryString>
