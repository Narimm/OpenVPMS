	<queryString language="SQL">
		<![CDATA[select
(
select count(*) as customerCount
from entities ep
left join entity_details ed1 on ed1.entity_id = ep.entity_id and ed1.name = 'createdDate'
where ep.arch_short_name = 'party.customerperson'
and ep.active = 1
and date(ed1.value) < $P{DateFrom}
) as activeCustomersStart,
(
select count(*) as customersCount
from entities ep
left join entity_details ed1 on ed1.entity_id = ep.entity_id and ed1.name = 'createdDate'
where ep.arch_short_name = 'party.customerperson'
and ep.active = 1
and date(ed1.value) < DATE_ADD($P{DateTo}, INTERVAL "23:59:59" HOUR_SECOND)
) as activeCustomersEnd,
(
select count(distinct(c.entity_id)) as count from entities c
where c.active = 0
and c.arch_short_name = 'party.customerPerson'
) as inActiveCustomers,
(
select count(distinct pc.entity_id) as customerCount
from acts a
left join participations pc on pc.act_id = a.act_id and pc.arch_short_name = 'participation.customer'
left join entities ec on ec.entity_id = pc.entity_id
left join participations pl on pl.act_id = a.act_id and pl.arch_short_name = 'participation.location'
left join entities el on el.entity_id = pl.entity_id
where a.arch_short_name like 'act.customerAccountCharges%'
and a.activity_start_time between $P{DateFrom} and DATE_ADD($P{DateTo}, INTERVAL "23:59:59" HOUR_SECOND)
and a.status = 'POSTED'
) as totalCustomerCount,
(
select count(distinct pc.entity_id) as customerCount
from acts a
left join participations pc on pc.act_id = a.act_id and pc.arch_short_name = 'participation.customer'
left join entities ec on ec.entity_id = pc.entity_id
left join participations pp on pp.entity_id = ec.entity_id
    and pp.arch_short_name = 'participation.customer'
    and pp.act_arch_short_name = 'act.customerAccountChargesInvoice'
    and pp.activity_start_time < $P{DateFrom}
where a.arch_short_name like 'act.customerAccountCharges%'
and a.activity_start_time between $P{DateFrom} and DATE_ADD($P{DateTo}, INTERVAL "23:59:59" HOUR_SECOND)
and pp.entity_id is null
and a.status = 'POSTED'
) as totalNewCustomerCount,

count(distinct pc.entity_id) as customerCount, count(*) as lineItemCount,
sum(if(f.credit,-(f.total - f.tax_amount),(f.total - f.tax_amount)))as lineItemTotalEx,
sum(if(f.credit,-(f.total),(f.total)))as lineItemTotalInc,
sum(if(f.credit,-(f.tax_amount),(f.tax_amount)))as lineItemTotalTax,
cast(sum(if(ai.arch_short_name = 'act.customerAccountCreditItem',1,0)) as signed) as creditItemCount,
sum(if(ai.arch_short_name = 'act.customerAccountCreditItem',if(f.credit,-(f.total - f.tax_amount),(f.total - f.tax_amount)),0))as creditItemTotalEx,
sum(if(ai.arch_short_name = 'act.customerAccountCreditItem',if(f.credit,-(f.total),(f.total)),0))as creditItemTotalInc,
sum(if(ai.arch_short_name = 'act.customerAccountCreditItem',if(f.credit,-(f.tax_amount),(f.tax_amount)),0))as creditItemTotalTax,
cast(sum(if((ai.arch_short_name = 'act.customerAccountCounterItem')
	and (ec.arch_short_name = 'party.organisationOTC'),1,0)) as signed) as counterItemCount,
sum(if((ai.arch_short_name = 'act.customerAccountCounterItem')
	and (ec.arch_short_name = 'party.organisationOTC'),
	if(f.credit,-(f.total - f.tax_amount),(f.total - f.tax_amount)),0))as counterItemTotalEx,
sum(if((ai.arch_short_name = 'act.customerAccountCounterItem')
	and (ec.arch_short_name = 'party.organisationOTC'),
	if(f.credit,-(f.total),(f.total)),0))as counterItemTotalInc,
sum(if((ai.arch_short_name = 'act.customerAccountCounterItem')
	and (ec.arch_short_name = 'party.organisationOTC'),
	if(f.credit,-(f.tax_amount),(f.tax_amount)),0))as counterItemTotalTax,
cast(sum(if((ai.arch_short_name = 'act.customerAccountInvoiceItem') or
	((ai.arch_short_name = 'act.customerAccountCounterItem') and (ec.name not like '%Counter Sales'))
	,1,0)) as signed) as invoiceItemCount,
sum(if((ai.arch_short_name = 'act.customerAccountInvoiceItem') or
	((ai.arch_short_name = 'act.customerAccountCounterItem') and (ec.name not like '%Counter Sales'))
	,if(f.credit,-(f.total - f.tax_amount),(f.total - f.tax_amount)),0))as invoiceItemTotalEx,
sum(if((ai.arch_short_name = 'act.customerAccountInvoiceItem') or
	((ai.arch_short_name = 'act.customerAccountCounterItem') and (ec.name not like '%Counter Sales'))
	,if(f.credit,-(f.total),(f.total)),0))as invoiceItemTotalInc,
sum(if((ai.arch_short_name = 'act.customerAccountInvoiceItem') or
	((ai.arch_short_name = 'act.customerAccountCounterItem') and (ec.name not like '%Counter Sales'))
	,if(f.credit,-(f.tax_amount),(f.tax_amount)),0))as invoiceItemTotalTax,
       ev.entity_id as clinician_id
from acts a
left join participations pc on pc.act_id = a.act_id and pc.arch_short_name = 'participation.customer'
left join entities ec on ec.entity_id = pc.entity_id
left join participations pl on pl.act_id = a.act_id and pl.arch_short_name = 'participation.location'
left join entities el on el.entity_id = pl.entity_id
left join act_relationships ar on ar.source_id = a.act_id
left join acts ai on ai.act_id = ar.target_id
left join financial_acts f on ai.act_id = f.financial_act_id
left join participations pv on pv.act_id = ai.act_id and pv.arch_short_name = 'participation.clinician'
left join entities ev on ev.entity_id = pv.entity_id
where a.arch_short_name like 'act.customerAccountCharges%'
and ai.arch_short_name like 'act.customerAccount%Item'
and f.total != 0
and a.activity_start_time between $P{DateFrom} and DATE_ADD($P{DateTo}, INTERVAL "23:59:59" HOUR_SECOND)
and a.status = 'POSTED'
group by pv.entity_id order by ev.name
;]]>
	</queryString>
