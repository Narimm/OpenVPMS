	<queryString language="SQL">
		<![CDATA[SELECT
     ifnull(clin.name,'--NO Clinician--') AS clinician_name, clin.description as clinician_descr,
     clin.entity_id as clinician_id,
	 ifnull(loc.name,'--NO Location--') as location,
	 ifnull(cust.name,'--NO Customer--') as custName, 
	 ifnull(cust.entity_id,0) as custId,
	 prod.name as prodName,
	 ifnull(ptyp.name,'--NO Product Type--') as prodType,
     if(credit,-(fa.total - fa.tax_amount),(fa.total - fa.tax_amount))as total,
     if(credit,-(fa.quantity),(fa.quantity))as qty,
	 a.activity_start_time as startTime, a.act_id as txnID
FROM
	acts a
    join act_relationships ar on ar.source_id = a.act_id and ar.arch_short_name like 'actRelationship.customerAccount%Item'
    join acts ai on ai.act_id = ar.target_id
    join financial_acts fa ON ai.act_id = fa.financial_act_id
    left join participations pc ON ai.act_id = pc.act_id and pc.arch_short_name = "participation.clinician"
    left join entities clin ON pc.entity_id = clin.entity_id
    left join participations pl ON a.act_id = pl.act_id and pl.arch_short_name = "participation.location"
    left join entities loc ON pl.entity_id = loc.entity_id
    left join participations pcu ON a.act_id = pcu.act_id and pcu.arch_short_name = "participation.customer"
    left join entities cust ON pcu.entity_id = cust.entity_id
    left join participations pr ON ai.act_id = pr.act_id and pr.arch_short_name = "participation.product"
    left join entities prod ON pr.entity_id = prod.entity_id
	left join entity_links et on et.source_id = prod.entity_id and et.arch_short_name = 'entityLink.productType'
	left join entities ptyp on ptyp.entity_id = et.target_id
WHERE
 a.activity_start_time between  $P{startDate} and date_add( $P{endDate} , INTERVAL "23:59:59" HOUR_SECOND)
 and a.arch_short_name like 'act.customerAccountCharges%'
 and a.status = 'POSTED'
 and prod.name like concat(ifnull($P{product},""),"%")
 and ifnull(ptyp.name,'-') like concat(ifnull($P{prodType},""),"%")
 and ifnull(clin.name,'-') like concat(ifnull($P{clinician},""),"%")
 and ifnull(loc.name,'-') like concat(ifnull($P{location},""),"%")
  
ORDER BY
     clinician_name ASC, location asc, prodType asc, prodName asc, startTime]]>
	</queryString>
