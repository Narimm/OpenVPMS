	<queryString language="SQL">
		<![CDATA[select 
loc.entity_id as locationID,
ifnull(locn.name,'--None--') as locationName, cl.entity_id as clinician_id, 
		ifnull(cld.value,ifnull(vet.name,'--None--')) as clinicianName,
		
      b.total as bTotal, b.txnCount as bTxnCount, b.custCount as bCustCount,
      a.total as aTotal, a.txnCount as aTxnCount, a.custCount as aCustCount,
      c.total as cTotal, c.txnCount as cTxnCount, c.custCount as cCustCount
from
(select u.entity_id  from entities u
join entity_classifications ec on ec.entity_id = u.entity_id
join lookups l on l.lookup_id = ec.lookup_id 

where u.arch_short_name = 'security.user'
and l.code = 'CLINICIAN'
union
select 0 as entity_id
) as cl

join ( select entity_id  from entities l 
		where arch_short_name = 'party.organisationLocation'
       union
         select 0 as entity_id
        ) as loc

left join entities vet on vet.entity_id = cl.entity_id
left join entity_details cld on cld.entity_id = cl.entity_id and cld.name = 'lastName'   
left join entities locn on locn.entity_id = loc.entity_id

left join (SELECT
     ifnull(pl.entity_id,0) as location_id, ifnull(pcl.entity_id,0) AS clinician_id, 
     sum(if(credit,-(f.total - f.tax_amount),(f.total - f.tax_amount)))as total, 
     count(a.act_id) as txnCount, count(distinct pcu.entity_id) as custCount
FROM
  acts a
  join financial_acts f ON a.act_id = f.financial_act_id
  left join participations pcu on pcu.act_id = a.act_id and pcu.arch_short_name = "participation.customer"
  left join participations pcl ON a.act_id = pcl.act_id and pcl.arch_short_name = "participation.clinician"
  left join participations pl ON a.act_id = pl.act_id and pl.arch_short_name = "participation.location"
WHERE
 a.activity_start_time >= $P{startDate}
 AND a.activity_start_time <= date_add($P{endDate}, INTERVAL "23:59:59" HOUR_SECOND)
 and a.status = 'POSTED'
 AND (a.arch_short_name LIKE "act.customerAccountCharges%")
GROUP BY pl.entity_id, pcl.entity_id
) as b on b.clinician_id = cl.entity_id and b.location_id = loc.entity_id
     
left join (SELECT
     ifnull(pl.entity_id,0) as location_id, ifnull(pcl.entity_id,0) AS clinician_id, 
     sum(if(credit,-(f.total - f.tax_amount),(f.total - f.tax_amount)))as total, 
     count(a.act_id) as txnCount, count(distinct pcu.entity_id) as custCount
FROM
  acts ai
  join financial_acts f ON ai.act_id = f.financial_act_id
  left join participations pcl ON ai.act_id = pcl.act_id 
            and pcl.arch_short_name = "participation.clinician"
  join act_relationships ar on ar.target_id = ai.act_id 
     and ar.arch_short_name like 'actRelationship.customerAccount%Item'
  join acts a on a.act_id = ar.source_id     
  left join participations pcu on pcu.act_id = a.act_id and pcu.arch_short_name = "participation.customer"
  left join participations pl ON a.act_id = pl.act_id and pl.arch_short_name = "participation.location"
WHERE
 a.activity_start_time >= $P{startDate}
 AND a.activity_start_time <= date_add($P{endDate}, INTERVAL "23:59:59" HOUR_SECOND)
 and a.status = 'POSTED'
 AND (ai.arch_short_name LIKE "act.customerAccount%Item")
GROUP BY pl.entity_id, pcl.entity_id
) as a 
     on a.clinician_id = cl.entity_id and a.location_id = loc.entity_id

left join (SELECT
     ca.location_id as location_id, ca.clinician_id as clinician_id, 
     sum(ca.total)as total, count(ca.act_id) as txnCount, count(distinct ca.cust_id) as custCount
FROM
(select
     ifnull(pl.entity_id,0) as location_id, ifnull(pcl.entity_id,0) AS clinician_id, 
     if(credit,-(f.total - f.tax_amount),(f.total - f.tax_amount)) as total, 
     a.act_id as act_id, pcu.entity_id as cust_id
from
  acts ai
  left join participations pcl ON ai.act_id = pcl.act_id 
            and pcl.arch_short_name = "participation.clinician"
  join act_relationships ar on ar.target_id = ai.act_id 
     and ar.arch_short_name like 'actRelationship.customerAccount%Item'
  join acts a on a.act_id = ar.source_id     
  left join participations pl ON a.act_id = pl.act_id and pl.arch_short_name = "participation.location"
  left join participations pcu on pcu.act_id = a.act_id and pcu.arch_short_name = "participation.customer"
  join financial_acts f ON a.act_id = f.financial_act_id
WHERE
 a.activity_start_time >= $P{startDate}
 AND a.activity_start_time <= date_add($P{endDate}, INTERVAL "23:59:59" HOUR_SECOND)
 and a.status = 'POSTED'
 AND (ai.arch_short_name LIKE "act.customerAccount%Item")
 group by a.act_id, location_id, clinician_id, pcu.entity_id

 ) as ca
GROUP BY ca.location_id,ca.clinician_id
) as c 
     on c.clinician_id = cl.entity_id and c.location_id = loc.entity_id

where 
 not(isnull(b.TxnCount) and isnull(a.txnCount) and isnull(c.txnCount))
 and  ifnull(cld.value,ifnull(vet.name,'-')) like concat(ifnull($P{clinician},""),"%")
 and ifnull(locn.name,'-') like concat(ifnull($P{location},""),"%")
  
order by   clinicianName,locationName]]>
