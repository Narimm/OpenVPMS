	<queryString language="SQL">
		<![CDATA[select
acType,custLocn,cid,name,current,30days,60days,90days, balance, unbilled,active
from
(select
acType,
custLocn,
cid,
name,
sum(if(days < 30,owing,0.00)) as current,
sum(if(days >=30 and days < 60, owing, 0.00)) as 30days,
sum(if(days >=60 and days < 90, owing, 0.00)) as 60days,
sum(if(days >=90,owing,0.00)) as 90days,
sum(owing) as balance,
sum(inProg) as unbilled,
active
from
(select e.entity_id as cid, e.name as name, ifnull(la.name,'--None--') as acType,
	 ifnull(loc.name,'--None--') as custLocn,
if (a.status = 'POSTED',if(f.credit,-(f.total-f.allocated_amount),(f.total-f.allocated_amount)),0.00) as owing,
if (a.status <> 'POSTED',if(f.credit,-(f.total-f.allocated_amount),(f.total-f.allocated_amount)),0.00) as inProg,
datediff(CURDATE() , a.activity_start_time) as days, 
e.active as active
from participations p
join acts a on a.act_id = p.act_id
join entities e on e.entity_id = p.entity_id
left join entity_classifications cl on cl.entity_id = e.entity_id
left join lookups la on la.lookup_id = cl.lookup_id and la.arch_short_name = 'lookup.customerAccountType'
left join entity_links el on el.source_id = e.entity_id and el.arch_short_name = 'entityLink.customerLocation'
left join entities loc on loc.entity_id = el.target_id
join financial_acts f on f.financial_act_id = a.act_id
where p.arch_short_name = "participation.customerAccountBalance"
  and ((isnull(cl.lookup_id) and isnull(la.lookup_id)) or ((not isnull(cl.lookup_id)) and (not isnull(la.lookup_id))))
  and   ifnull(la.name,"--") like concat(ifnull($P{acType},""),"%") 
  and   ifnull(loc.name,"--") like concat(ifnull($P{location},""),"%")
  and (e.active ||  $P{IncludeInactive} )
) as accountbalances
group by cid
) acb
where 
  balance <> 0

order by custLocn,acType,name;]]>
