}
PETBREED.setString(trim(PETBREED.getString()))
COLOUR.setString(wordUtils.capitalizeFully(trim(COLOUR.getString())))

&#47;&#47; Transform Sex
if (PETSEX.getString() == &quot;F&quot;)
