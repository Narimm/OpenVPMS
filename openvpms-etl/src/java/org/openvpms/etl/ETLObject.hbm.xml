<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.openvpms.etl.ETLObject" table="etl_object">
        <cache usage="read-write"/>
        <id name="objectId">
            <column name="object_id"/>
            <generator class="native"/>
        </id>
        <property name="legacyId" access="property" column="legacy_id"
                  type="string" length="32" index="etl_object_legacy_id_idx"/>
        <property name="archetype" access="property" column="archetype"
                  type="string" length="32"/>
        <bag name="nodes" access="property" inverse="true"
             cascade="all,delete-orphan" lazy="false">
            <key column="object_id"/>
            <one-to-many class="org.openvpms.etl.ETLNode"/>
        </bag>
    </class>

</hibernate-mapping>