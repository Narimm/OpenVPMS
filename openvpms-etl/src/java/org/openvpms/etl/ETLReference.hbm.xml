<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.openvpms.etl.ETLReference" table="etl_reference">
        <cache usage="read-write"/>
        <id name="referenceId">
            <column name="reference_id"/>
            <generator class="native"/>
        </id>
        <version name="version" access="property" type="long"/>
        <many-to-one name="object" column="object_id"
                     class="org.openvpms.etl.ETLObject"/>
        <property name="reference" access="property" column="reference"
                  type="string" length="64"/>
    </class>

</hibernate-mapping>