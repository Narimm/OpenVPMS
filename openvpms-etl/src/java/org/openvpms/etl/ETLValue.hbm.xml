<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.openvpms.etl.ETLValue" table="etl_value">
        <cache usage="read-write"/>
        <id name="valueId">
            <column name="value_id"/>
            <generator class="native"/>
        </id>
        <property name="objectId" access="property" column="object_id"
                  type="string" length="32" index="etl_value_object_id_idx"/>
        <property name="legacyId" access="property" column="legacy_id"
                  type="string" length="32" index="etl_value_ref_idx"/>
        <property name="archetype" access="property" type="string" length="100"
                  index="etl_value_ref_idx"/>
        <property name="reference" column="is_reference" type="boolean"/>
        <property name="name" access="property" column="name" type="string"
                  length="32"/>
        <property name="value" access="property" column="value"
                  type="string" length="4096"/>
        <property name="removeDefaultObjects" access="property"
                  column="remove_default_objects" type="boolean"/>
    </class>
</hibernate-mapping>